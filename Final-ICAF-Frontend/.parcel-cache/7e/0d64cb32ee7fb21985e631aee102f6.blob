html {
        scroll-behavior: smooth;
      }

      #myBtn {
        display: none;
        position: fixed;
        bottom: 20px;
        right: 30px;
        z-index: 99;
        font-size: 25px;
        border: none;
        outline: none;
        background-color: rgb(0, 75, 136);
        color: white;
        cursor: pointer;
        padding: 5px 10px;
        border-radius: 10%;
      }

      #myBtn:hover {
        background-color: #555;
      }